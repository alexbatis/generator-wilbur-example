{
  "name": "university-app",
  "version": "1.0.0",
  "description": "An nodejs application generated by wilbur",
  "main": "index.js",
  "scripts": {
    "start": "cd build && node main",
    "start:server": "node main | pino -o 'short'",
    "compile:local": "node build-scripts/build.js && backpack build",
    "compile:dev": "node build-scripts/build.dev.js && backpack build",
    "compile:test": "node build-scripts/build.test.js && backpack build",
    "compile:qa": "node build-scripts/build.qa.js && backpack build",
    "compile:prod": "node build-scripts/build.prod.js && backpack build",
    "serve": "node build-scripts/build.js && backpack dev | pino -o 'short'",
    "serve:dev": "node build-scripts/build.dev.js && backpack dev | pino -o 'short'",
    "serve:test": "node build-scripts/build.test.js && backpack dev | pino -o 'short'",
    "serve:qa": "node build-scripts/build.qa.js && backpack dev | pino -o 'short'",
    "serve:prod": "node build-scripts/build.prod.js && backpack dev | pino -o 'short'",
    "test": "node build-scripts/build.js && mocha test/**/*.ts --exit",
    "test:watch": "node build-scripts/build.js && mocha -r ts-node/register mocha test/**/*.ts  --watch --watch-extensions ts",
    "test:report": "node build-scripts/build.js && mocha test/**/*.ts --reporter mochawesome --exit",
    "test:coverage": "nyc mocha test/**/*.ts --exit --require ts-node/register --require source-map-support/register --recursive"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "class-validator": "^0.9.1",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-validator": "^5.0.3",
    "httpntlm": "^1.7.5",
    "install": "^0.11.0",
    "istanbul": "^0.4.5",
    "js-yaml": "^3.12.0",
    "json-refs": "^2.1.6",
    "json2typescript": "^1.0.5",
    "jsonwebtoken": "^8.2.1",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.10",
    "mongoose": "^5.0.11",
    "nock": "^9.2.5",
    "openid-client": "^2.0.1",
    "passport": "^0.4.0",
    "pino": "^4.7.3",
    "portscanner": "^2.1.1",
    "reflect-metadata": "^0.1.12",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "source-map-support": "^0.4.15",
    "supertest-session": "^3.2.0",
    "swagger-express-middleware": "^1.0.0-alpha.12",
    "typegoose": "^5.0.0",
    "types-installer": "^1.4.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/chai": "^4.1.2",
    "@types/cookie-parser": "^1.4.1",
    "@types/cookie-session": "^2.0.34",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.11.1",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/jwt-simple": "^0.5.33",
    "@types/lodash": "^4.14.108",
    "@types/mocha": "^5.0.0",
    "@types/mongoose": "^5.0.7",
    "@types/passport": "^0.4.5",
    "@types/pino": "^4.7.1",
    "@types/portscanner": "^2.1.0",
    "@types/request-promise": "^4.1.41",
    "@types/shelljs": "^0.7.8",
    "@types/source-map-support": "^0.4.0",
    "@types/supertest": "^2.0.4",
    "@types/swagger-express-middleware": "^1.0.8",
    "awesome-typescript-loader": "^3.4.1",
    "backpack-core": "^0.4.3",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "mochawesome": "^3.0.2",
    "nyc": "^11.6.0",
    "shelljs": "^0.7.8",
    "supertest": "^3.0.0",
    "ts-node": "^5.0.1",
    "tsconfig-paths": "^3.5.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.3.2",
    "typescript": "^3.0.1"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Alex Batis",
    "email": "Alexb@me.com",
    "github": "https://github.com/alexbatis"
  }
}
